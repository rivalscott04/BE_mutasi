{
  "name": "amut-backend",
  "version": "1.0.0",
  "description": "Backend API for AMUT system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:full": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:run-migration": "ts-node src/scripts/run-migration.ts",
    "db:seed": "ts-node src/scripts/run-seeder.ts",
    "db:admin-wilayah": "ts-node src/scripts/run-admin-wilayah-migration.ts",
    "db:seed-admin-wilayah": "ts-node src/scripts/run-admin-wilayah-seeder.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": ["express", "sequelize", "typescript", "api"],
  "author": "AMUT Team",
  "license": "MIT"
}
